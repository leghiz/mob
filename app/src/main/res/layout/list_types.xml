<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/list"
    android:paddingLeft="15dp"
    android:paddingVertical="5dp"
    android:layout_marginHorizontal="5dp"
    android:layout_marginVertical="10dp"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/typeText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="18sp"
        android:textColor="@color/black"
        android:paddingRight="20dp"
        app:layout_constraintTop_toTopOf="@id/ic_cyclists"
        app:layout_constraintBottom_toBottomOf="@id/ic_cyclists"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/ic_cyclists"
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:src="@drawable/ic_cyclists"
        app:layout_constraintLeft_toRightOf="@id/typeText"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>